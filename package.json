{
  "name": "tree-sitter-apex",
  "version": "0.0.1",
  "description": "A tree-sitter for the apex programming language",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "apex",
    "tree-sitter"
  ],
  "author": "Edwin Scharfe",
  "license": "MIT",
  "scripts": {
    "build": "tree-sitter generate && node-gyp rebuild",
    "test": "tree-sitter test",
    "build-test": "tree-sitter generate && node-gyp build && tree-sitter test"
  },
  "dependencies": {
    "nan": "^2.16.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ree-sitter/ree-sitter-apex.git"
  },
  "tree-sitter": [
    {
      "scope": "source.cls",
      "file-types": [
        "cls"
      ]
    }
  ]
}
